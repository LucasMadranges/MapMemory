generator client {
  provider = "prisma-client-js"
  output   = "../../../../../node_modules/.prisma/client" // Mettre dans un dossier plus accessible
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_USERS")
}

model User {
  id    String @id @default(uuid())
  name  String
  email String @unique
}
